<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 没有焦点时的背景图片 -->
    <item android:drawable="@drawable/dialog_button_background" android:state_window_focused="false"/>
    <!-- 获得焦点时的图片背景 -->
    <item android:drawable="@drawable/cs_channel_focus" android:state_focused="true"/>
    <item android:drawable="@drawable/cs_channel_focus" android:state_selected="true"/>
    <!-- 默认时的背景图片 -->
    <item android:drawable="@drawable/dialog_button_background"/>

</selector>